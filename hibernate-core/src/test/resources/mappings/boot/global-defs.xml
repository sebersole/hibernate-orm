<entity-mappings xmlns="http://www.hibernate.org/xsd/orm/mapping" version="3.1">
    <java-type class="java.lang.String" descriptor="org.hibernate.orm.test.boot.annotations.bind.global.CustomStringJavaType"/>
    <jdbc-type descriptor="org.hibernate.orm.test.boot.annotations.bind.global.CustomVarcharJdbcType"/>
    <user-type class="java.util.BitSet" descriptor="org.hibernate.orm.test.boot.annotations.bind.global.BitSetUserType"/>
    <conversion converter="org.hibernate.orm.test.mapping.mutability.converted.MapConverter" class="java.util.Map"/>

    <filter-def name="filter1"/>
    <sequence-generator name="seq1"/>

    <named-query name="query1">
        <query>from BasicEntity</query>
    </named-query>

    <named-native-query name="query2">
        <query>select * from BasicEntity</query>
    </named-native-query>

    <converter class="org.hibernate.orm.test.mapping.mutability.converted.MapConverter" />
</entity-mappings>
